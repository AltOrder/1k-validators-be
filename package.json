{
  "name": "1k-validators-be",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "docker:start": "yarn build:clean:all && docker-compose up -d --build",
    "lint:all": "yarn workspaces foreach run lint",
    "build:all": "yarn workspaces foreach run build",
    "build:clean:all": "yarn workspaces foreach run clean:build",
    "start:dev:gateway": "yarn workspace @1kv/gateway run start:dev",
    "start:dev:core": "yarn workspace @1kv/core run start:dev"
  },
  "dependencies": {
  }
}
